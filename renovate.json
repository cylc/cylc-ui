{
  "extends": [
    "config:js-app"
  ],
  "labels": ["dependencies", "javascript"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^@lumino/"],
      "groupName": "lumino packages"
    },
    {
      "matchPackagePatterns": ["^eslint"],
      "groupName": "eslint packages"
    }
  ],
  "vulnerabilityAlerts": {
    "addLabels": ["security"]
  }
}
