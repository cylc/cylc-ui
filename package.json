    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "lodash": "4.17.21",
    "markdown-it": "13.0.1",
    "mitt": "3.0.0",
    "nprogress": "1.0.0-1",
    "preact": "10.15.1",
    "preact-compat": "3.19.0",
    "subscriptions-transport-ws": "0.11.0",
    "svg-pan-zoom": "3.6.1",
    "vue": "3.2.47",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.6",
    "vue-the-mask": "0.11.1",
    "vue3-apexcharts": "1.4.1",
    "vuetify": "3.1.7",
    "vuex": "4.1.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.10.8",
