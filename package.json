    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.14.1",
    "eslint-plugin-vuetify": "^2.0.1",
    "express": "^4.17.2",
    "express-ws": "^5.0.2",
    "graphql-ws": "^5.12.1",
    "istanbul-lib-coverage": "^3.0.0",
    "jsdom": "^22.0.0",
    "json-server": "^0.17.0",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "sass": "~1.63.0",
